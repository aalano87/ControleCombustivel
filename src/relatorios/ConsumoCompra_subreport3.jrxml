	<queryString>
		<![CDATA[select
sum(e.qtde_litros) as litros_comprados, p.nome as nome_comprador, e.mes
from entrada e
inner join proprietario p
on e.idproprietario = p.idproprietario
where e.data between $P{Data1} and $P{Data2}
group by p.nome, e.mes]]>
